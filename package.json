{
  "name": "discord-bot",
  "version": "3.0.0",
  "description": "The CodeSupport Discord bot",
  "main": "./build/app.js",
  "scripts": {
    "start": "node -r dotenv/config ./build/index.js",
    "build": "tsc",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "test": "ts-mocha test/**/*Test.ts test/**/**/*Test.ts test/appTest.ts --exit",
    "lint": "eslint src --ext .js,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesupport/discord-bot.git"
  },
  "keywords": [
    "codesupport",
    "discord",
    "bot"
  ],
  "author": "The CodeSupport Community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codesupport/discord-bot/issues"
  },
  "homepage": "https://github.com/codesupport/discord-bot#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "discord.js": "^12.2.0",
    "twitter": "^1.7.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/assert": "^1.4.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "@types/sinon": "^9.0.0",
    "@types/twitter": "^1.7.0",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-codesupport": "^1.0.2",
    "mocha": "^7.1.2",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "ts-mocha": "^7.0.0",
    "typescript": "^3.8.3"
  }
}
